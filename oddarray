package testing;

public class oddarray {
public static void main(String[]args)
{
	int n[]=new int[7];
	n[0]=22;
	n[1]=44;
	n[2]=34;
	n[3]=46;
	n[4]=54;
	n[5]=66;
	n[6]=32;
	for(int i=0;i<n.length;i++)
	{
		
		if(n[i]%2==0)
		{
			System.out.println(n[i]+" "+"This number is even");
		}else
		{
			System.out.println(n[i]+" "+"This number is odd");
		}
	}
}
}
