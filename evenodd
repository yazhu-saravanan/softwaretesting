package testing;

import java.util.Scanner;

public class count {
public static void main(String[]args)
	
	{
		Scanner r=new Scanner(System.in);
		System.out.println("Enter the Number;");
		int n=r.nextInt();
		int count=0;
		System.out.println("This is even number");
		for(int i=1;i<=n;i++)
		{
			if(i % 2==0)
			{
			count=count+1;
			}
		
	    }System.out.println(count);
	}


}

