package testing;
import java.util.List;
import java.util.ArrayList;
public class remove {
    public static void main(String[]args)
    {
    	List<Integer>m=new ArrayList<Integer>();
    	m.add(10);
    	m.add(20);
    	m.add(30);
    	m.add(20);
    	m.add(40);
    	m.remove(3);
    	System.out.println(m);
    }
}
