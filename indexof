package testing;

public class indexof {
	public static void main(String[]args)
	{
		String s1="He is my man";
		int m=s1.indexOf("i");
		System.out.println(m);
		int m1=s1.indexOf("t");
		System.out.println(m1);
		int m2=s1.indexOf("m");
		System.out.println(m2);
		int m3=s1.lastIndexOf("m");
		System.out.println(m3);
	}

}
