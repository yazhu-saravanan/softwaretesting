package testing;
import java.util.List;
import java.util.ArrayList;

public class forget {
	public static void main(String[]args)
	{
		List<Integer>n=new ArrayList<Integer>();
		n.add(10);
		n.add(20);
		n.add(30);
		n.add(40);
		for(int i=0;i<n.size();i++)
		{
			System.out.println(n.get(i));
		}
		
	}

}
